{
  "name": "eslint-plugin-mock-typed",
  "version": "0.0.9",
  "description": "ESlint rules for mock-typed, a tiny TypeScript library for type-safe function mocks in unit tests",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist package.tgz tsconfig.tsbuildinfo",
    "build": "tsc -b --clean",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "build:watch": "pnpm run build && tsc -b --watch",
    "prepack": "pnpm run clean && pnpm run build",
    "lint": "eslint 'src/**/*.ts'",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milichev/mock-typed.git",
    "directory": "packages/eslint-plugin-mock-typed"
  },
  "author": {
    "name": "Vadym Milichev",
    "email": "vadim.milichev@gmail.com"
  },
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "dependencies": {
    "mock-typed": "workspace:*",
    "tiny-invariant": "^1.3.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.9.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@typescript-eslint/rule-tester": "^8.13.0",
    "@typescript-eslint/utils": "^8.13.0",
    "eslint": "^9.14.0",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "eslint": "^9"
  }
}
